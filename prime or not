#include<stdio.h>
int main()
{
	int i,number,count=0;
	printf("enter a number to check");
	scanf("%d",&number);
	for(i=2;i<=number/2;i++)
	{
		if(number%i==0)
		{
			count==0;
		break;
		}
		if(count==0 &number!=1)
		{
			printf("%d is a prime number",number);
		}
		else
		{
			printf("%d is not prime");
		}
	}	
	return 0;
	
	
}
